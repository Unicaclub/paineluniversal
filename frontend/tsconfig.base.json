{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020","DOM","DOM.Iterable"],
      "types": ["node"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "moduleDetection": "force",
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      }
    }
  }
  